<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-schema="BDM" match-name="BDM_HR_COMPANY"/>
  <table name="BDM_HR_COMPANY">
  	<primary-key property="compId">
  		<generator class="sequence">
  			<param name="sequence_name">BDM_HR_COMPANY_SEQ</param>
  		</generator>
  	</primary-key>
  </table>
</hibernate-reverse-engineering>
