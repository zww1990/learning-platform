<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-schema="APOLLOCONFIG" match-name="APP"/>
  <table-filter match-schema="APOLLOCONFIG" match-name="APPNAMESPACE"/>
  <table-filter match-schema="APOLLOCONFIG" match-name="COMMIT"/>
  <table-filter match-schema="APOLLOCONFIG" match-name="GRAYRELEASERULE"/>
  <table-filter match-schema="APOLLOCONFIG" match-name="INSTANCE"/>
  <table-filter match-schema="APOLLOCONFIG" match-name="INSTANCECONFIG"/>
  <table-filter match-schema="APOLLOCONFIG" match-name="ITEM"/>
  <table-filter match-schema="APOLLOCONFIG" match-name="NAMESPACE"/>
  <table-filter match-schema="APOLLOCONFIG" match-name="NAMESPACELOCK"/>
  <table-filter match-schema="APOLLOCONFIG" match-name="RELEASE"/>
  <table-filter match-schema="APOLLOCONFIG" match-name="RELEASEHISTORY"/>
  <table-filter match-schema="APOLLOCONFIG" match-name="RELEASEMESSAGE"/>
  <table-filter match-schema="APOLLOCONFIG" match-name="SERVERCONFIG"/>
  <table-filter match-schema="APOLLOCONFIG" match-name="audit"/>
  <table-filter match-schema="APOLLOCONFIG" match-name="cluster"/>
  <table name="APP">
  	<primary-key property="id" id-class="java.lang.Long">
  		<generator class="SEQUENCE">APP_SEQ</generator>
  	</primary-key>
  	<column name="ISDELETED" property="deleted" type="boolean"></column>
  </table>
</hibernate-reverse-engineering>