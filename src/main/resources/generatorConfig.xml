<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<properties resource="generatorConfig4Maven.properties" />
	<classPathEntry location="${location}" />
	<context id="wiwj" targetRuntime="MyBatis3">
		<plugin type="org.mybatis.generator.plugins.MySerializablePlugin" />
		<plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="${driverClassName}" connectionURL="${url}" userId="${username}" password="${password}" />
		<javaTypeResolver type="org.mybatis.generator.internal.types.MyJavaTypeResolverImpl">
			<property name="forceBigDecimals" value="false"/>
		</javaTypeResolver>
		<javaModelGenerator targetPackage="${javaModelGenerator.targetPackage}" targetProject="${javaModelGenerator.targetProject}">
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="${sqlMapGenerator.targetPackage}" targetProject="${sqlMapGenerator.targetProject}">
		</sqlMapGenerator>
		<javaClientGenerator targetPackage="${javaClientGenerator.targetPackage}" targetProject="${javaClientGenerator.targetProject}" type="XMLMAPPER">
		</javaClientGenerator>
		<table tableName="BDM_SHOPS" enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false" enableUpdateByExample="false">
			<generatedKey column="SHOP_ID" sqlStatement="select BDM_SHOPS_SEQ.nextval from dual"/>
		</table>
	</context>
</generatorConfiguration>